<!DOCTYPE html>
<html lang="en">

<head>
     <%- include('../partials/frontend-head', { title: 'Center Leaders' }) %>
</head>

<body>
    <!-- Preloader -->
    <% include ../partials/navbar.ejs %>

    <!-- ***** Breadcumb Area Start ***** -->
    <section class="breadcumb-area bg-img gradient-background-overlay">
        <div class="container h-100">
            <div class="row h-100 align-items-center">
                <div class="col-12">
                    <div class="breadcumb-content">
                        <h3 class="breadcumb-title">Centre Leaders</h3>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ***** Breadcumb Area End ***** -->
    <section class="our-skills-area text-center section-padding-50-0">
        <div class="container">
            <div class="row">
            <% for (let i = 0; i < team.length; i++) { %>
                <div class="col-12 col-sm-3 col-md-3 col-lg-4 team mb-5">
                    <img src="<%= team[i].postImage %>" alt="<%= team[i].name %>">
                    <p class="fa-lg mt-3 mb-1" style="line-height:1.3"><%= team[i].name %></p>
                    <p><%= team[i].position %></p>
                </div>
            <% } %>
            </div>
        </div>
    </section>

    <% include ../partials/footer.ejs %>

    <% include ../partials/frontend-scripts %>

</body>
</html>